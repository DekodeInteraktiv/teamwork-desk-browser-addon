{
  "manifest_version": 2,
  "name": "Teamwork Note",
  "version": "1.0",
  "description": "Help you fetch a note from Teamwork single ticket view",
  "icons": {
    "64": "icons/icon-64.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon-64.png",
    "default_title": "Teamwork Note",
    "default_popup": "popup/index.html"
  }
}